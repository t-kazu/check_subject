<table border align="center">
    <caption>
        <font size="5"><strong>授業管理表</strong></font>
    </caption>
    <tr>
        <th width="8%">日付</th><th width="6%">分類</th><th width="6%">時限</th><th width="16%">授業名</th><th width="16%">教室</th><th width="20%">備考</th>
    </tr>
    <% @subjects.each do |subject| %>
        <tr>
            <td><%= subject.date %></td>
            <td><%= subject.attribute %></td>
            <td><%= subject.time %></td>
            <td><%= subject.name %></td>
            <td><%= subject.room %></td>
            <td><%= subject.other %></td>
        </tr>
    <% end %>
</table>
